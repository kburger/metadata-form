{
    "title": "Distribution",
    "type": "http://www.w3.org/ns/dcat#Distribution",
    "namespaces": {
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "dct": "http://purl.org/dc/terms/",
        "dcat": "http://www.w3.org/ns/dcat#",
        "fdp": "http://rdf.biosemantics.org/ontologies/fdp-o#",
        "datacite": "http://purl.org/spar/datacite/"
    },
    "components": {
        "dct-title": {
            "name": "Title",
            "url": "http://purl.org/dc/terms/title",
            "type": "text",
            "description": "A name given to the distribution.",
            "required": true
        },
        "dct-issued": {
            "name": "Release date",
            "url": "http://purl.org/dc/terms/issued",
            "type": "text",
            "description": "Date of formal issuance (e.g., publication) of the distribution.",
            "required": true,
            "generated": true
        },
        "dct-modified": {
            "name": "Update/modification date",
            "url": "http://purl.org/dc/terms/modified",
            "type": "text",
            "description": "Most recent date on which the distribution was changed, updated or modified.",
            "required": true,
            "generated": true
        },
        "dct-license": {
            "name": "License",
            "url": "http://purl.org/dc/terms/license",
            "type": "url",
            "description": "This links to the license document under which the distribution is made available.",
            "required": true,
            "autocomplete": {
                "constant": "license"
            }
        },
        "dct-hasversion": {
            "name": "Has version",
            "url": "http://purl.org/dc/terms/hasVersion",
            "type": "text",
            "description": "The version number for this resource",
            "required": true
        },
        "dct-rights": {
            "name": "Rights",
            "url": "http://purl.org/dc/terms/rights",
            "type": "url",
            "description": "Information about rights held in and over the distribution.",
            "required": false
        },
        "dct-description": {
            "name": "Description",
            "url": "http://purl.org/dc/terms/description",
            "type": "text",
            "description": "free-text account of the distribution.",
            "required": false
        },
        "rdfs-label": {
            "name": "Label",
            "url": "http://www.w3.org/2000/01/rdf-schema#label",
            "type": "text",
            "description": "Label that indicates the thing",
            "required": true
        },
        "dcat-accessurl": {
            "name": "Access URL",
            "url": "http://www.w3.org/ns/dcat#accessURL",
            "type": "url",
            "description": "A landing page, feed, SPARQL endpoint or other type of resource that gives access to the distribution of the dataset.",
            "required": true,
            "alternative": "dcat-downloadurl"
        },
        "dcat-downloadurl": {
            "name": "Download URL",
            "url": "http://www.w3.org/ns/dcat#downloadURL",
            "type": "url",
            "description": "A file that contains the distribution of the dataset in a given format.",
            "required": true,
            "alternative": "dcat-accessurl"
        },
        "dcat-mediatype": {
            "name": "Media type",
            "url": "http://www.w3.org/ns/dcat#mediaType",
            "type": "text",
            "description": "The media type of the distribution as defined by IANA.",
            "required": true
        },
        "dcat-format": {
            "name": "Format",
            "url": "http://www.w3.org/ns/dcat#format",
            "type": "text",
            "description": "The file format of the distribution.",
            "required": false
        },
        "dcat-bytesize": {
            "name": "Byte size",
            "url": "http://www.w3.org/ns/dcat#byteSize",
            "type": "text",
            "description": "The size of a distribution in bytes.",
            "required": false
        },
        "fdp-metadataidentifier": {
            "name": "Metadata identifier",
            "url": "http://rdf.biosemantics.org/ontologies/fdp-o#metadataIdentifier",
            "type": "url",
            "description": "Identifier of the metadata entry",
            "required": true,
            "nested": {
                "type": "http://purl.org/spar/datacite/ResourceIdentifier",
                "components": {
                    "metadata-dct-identifier": {
                        "name": "Identifier",
                        "url": "http://purl.org/dc/terms/identifier",
                        "type": "text",
                        "description": "Unique identifier for this resource",
                        "required": true
                    }
                }
            }
        },
        "fdp-metadataissued": {
            "name": "Release date",
            "url": "http://rdf.biosemantics.org/ontologies/fdp-o#metadataIssued",
            "type": "text",
            "description": "Date of formal issuance (e.g., publication) of the repository.",
            "required": true,
            "generated": true
        },
        "fdp-metadatamodified": {
            "name": "Update/modification date",
            "url": "http://rdf.biosemantics.org/ontologies/fdp-o#metadataModified",
            "type": "text",
            "description": "Most recent date on which the repository was changed, updated or modified.",
            "required": true,
            "generated": true
        }
    }
}