{
    "title": "Catalog",
    "type": "http://www.w3.org/ns/dcat#Catalog",
    "namespaces": {
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "dct": "http://purl.org/dc/terms/",
        "dcat": "http://www.w3.org/ns/dcat#",
        "foaf": "http://xmlns.com/foaf/0.1/",
        "fdp": "http://rdf.biosemantics.org/ontologies/fdp-o#",
        "datacite": "http://purl.org/spar/datacite/"
    },
    "components": {
        "dct-title": {
            "name": "Title",
            "url": "http://purl.org/dc/terms/title",
            "type": "text",
            "description": "A name given to the catalog.",
            "required": true
        },
        "dct-issued": {
            "name": "Release date",
            "url": "http://purl.org/dc/terms/issued",
            "type": "text",
            "description": "Date of formal issuance (e.g., publication) of the catalog.",
            "required": true,
            "generated": true
        },
        "dct-modified": {
            "name": "Update/modification date",
            "url": "http://purl.org/dc/terms/modified",
            "type": "text",
            "description": "Most recent date on which the catalog was changed, updated or modified.",
            "generated": true
        },
        "dct-hasversion": {
            "name": "Has version",
            "url": "http://purl.org/dc/terms/hasVersion",
            "type": "text",
            "description": "The version number for this resource",
            "required": true
        },
        "dct-publisher": {
            "name": "Publisher",
            "url": "http://purl.org/dc/terms/publisher",
            "type": "url",
            "description": "The entity responsible for making the catalog online.",
            "required": true,
            "nested": {
                "type": "http://xmlns.com/foaf/0.1/Organization",
                "components": {
                    "institure-foaf-name": {
                        "name": "Name",
                        "url": "http://xmlns.com/foaf/0.1/name",
                        "type": "text",
                        "description": "Name of the insitution",
                        "required": true
                    }
                }
            }
        },
        "dct-description": {
            "name": "Description",
            "url": "http://purl.org/dc/terms/description",
            "type": "text",
            "description": "A free-text account of the catalog.",
            "required": false
        },
        "dct-language": {
            "name": "Language",
            "url": "http://purl.org/dc/terms/language",
            "type": "url",
            "description": "The language of the catalog. This refers to the language used in the textual metadata describing titles, descriptions, etc. of the datasets in the catalog.",
            "multiple": true,
            "required": false,
            "autocomplete": {
                "constant": "language"
            }
        },
        "dct-license": {
            "name": "License",
            "url": "http://purl.org/dc/terms/license",
            "type": "url",
            "description": "This links to the license document under which the catalog is made available and not the datasets. Even if the license of the catalog applies to all of its datasets and distributions, it should be replicated on each distribution.",
            "required": false,
            "autocomplete": {
                "constant": "license"
            }
        },
        "dct-rights": {
            "name": "Rights",
            "url": "http://purl.org/dc/terms/rights",
            "type": "url",
            "description": "This describes the rights under which the catalog can be used/reused and not the datasets. Even if theses rights apply to all the catalog datasets and distributions, it should be replicated on each distribution.",
            "required": false
        },
        "rdfs-label": {
            "name": "Label",
            "url": "http://www.w3.org/2000/01/rdf-schema#label",
            "type": "text",
            "description": "Label that indicates the thing",
            "required": false
        },
        "foaf-homepage": {
            "name": "Homepage",
            "url": "http://xmlns.com/foaf/0.1/homepage",
            "type": "url",
            "description": "The homepage of the catalog.",
            "required": false
        },
        "dcat-dataset": {
            "name": "Dataset",
            "url": "http://www.w3.org/ns/dcat#dataset",
            "type": "url",
            "description": "A dataset that is part of the catalog.",
            "multiple": true,
            "required": true,
            "generated": true
        },
        "dcat-themetaxonomy": {
            "name": "Theme taxonomy",
            "url": "http://www.w3.org/ns/dcat#themeTaxonomy",
            "type": "url",
            "description": "The knowledge organization system (KOS) used to classify catalog's datasets.",
            "required": true
        },
        "fdp-metadataidentifier": {
            "name": "Metadata identifier",
            "url": "http://rdf.biosemantics.org/ontologies/fdp-o#metadataIdentifier",
            "type": "url",
            "description": "Identifier of the metadata entry",
            "required": true,
            "nested": {
                "type": "http://purl.org/spar/datacite/ResourceIdentifier",
                "components": {
                    "metadata-dct-identifier": {
                        "name": "Identifier",
                        "url": "http://purl.org/dc/terms/identifier",
                        "type": "text",
                        "description": "Unique identifier for this resource",
                        "required": true
                    }
                }
            }
        },
        "fdp-metadataissued": {
            "name": "Release date",
            "url": "http://rdf.biosemantics.org/ontologies/fdp-o#metadataIssued",
            "type": "text",
            "description": "Date of formal issuance (e.g., publication) of the repository.",
            "required": true,
            "generated": true
        },
        "fdp-metadatamodified": {
            "name": "Update/modification date",
            "url": "http://rdf.biosemantics.org/ontologies/fdp-o#metadataModified",
            "type": "text",
            "description": "Most recent date on which the repository was changed, updated or modified.",
            "required": true,
            "generated": true
        }
    }
}